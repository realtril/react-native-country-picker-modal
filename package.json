{
  "name": "@realtril/react-native-country-picker-modal",
  "main": "src/index.tsx",
  "types": "src/index.tsx",
  "version": "1.0.3",
  "scripts": {
    "start": "expo start",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "lint": "expo lint",
    "data:image": "rm -rf ./src/assets/data/countries.json && node ./scripts/transform-world-countries.js > ./src/assets/data/countries.json",
    "data:emoji": "rm -rf ./src/assets/data/countries-emoji.json && node ./scripts/transform-world-countries.js --emoji > ./src/assets/data/countries-emoji.json",
    "data:cca2": "rm -rf ./src/assets/data/cca2.json && node ./scripts/transform-world-countries.js --cca2 > ./src/assets/data/cca2.json",
    "data:regions": "rm -rf ./src/assets/data/regions.json && node ./scripts/transform-world-countries.js --regions > ./src/assets/data/regions.json",
    "data:subregions": "rm -rf ./src/assets/data/subregions.json && node ./scripts/transform-world-countries.js --subregions > ./src/assets/data/subregions.json"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@callstack/react-theme-provider": "^3.0.9",
    "@expo/vector-icons": "^14.0.2",
    "@react-navigation/bottom-tabs": "^7.2.0",
    "@react-navigation/native": "^7.0.14",
    "@realtril/react-native-country-picker-modal": "./",
    "expo": "~52.0.46",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-font": "~13.0.4",
    "expo-haptics": "~14.0.1",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "~0.29.24",
    "expo-status-bar": "~2.0.1",
    "expo-symbols": "~0.2.2",
    "expo-system-ui": "~4.0.9",
    "expo-web-browser": "~14.0.2",
    "fuse.js": "^3.4.5",
    "node-emoji": "^1.11.0",
    "react": "18.3.1",
    "react-async-hook": "^4.0.0",
    "react-dom": "18.3.1",
    "react-native": "0.76.9",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-reanimated": "~3.16.1",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "world-countries": "^5.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@types/jest": "^29.5.12",
    "@types/node-emoji": "^1.8.2",
    "@types/react": "~18.3.12",
    "@types/react-test-renderer": "^18.3.0",
    "jest": "^29.2.1",
    "jest-expo": "~52.0.6",
    "react-test-renderer": "18.3.1",
    "typescript": "^5.3.3"
  }
}
